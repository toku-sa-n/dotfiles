snippet if "if ... end"
if ${1}
	${2}
end
${0}
endsnippet

snippet tt "t.times do ... end"
${1}.times do${2: |${3:i}|}
	${4}
end
${0}
endsnippet

snippet et "(start...end).each do ... end"
(${1}..${2}).each do${3: |${4:i}|}
	${5}
end
${0}
endsnippet
snippet un "unless ... end"
unless ${1}
	${2}
end
${0}
endsnippet
