!! $HOME/.Xresources

!!
!! rxvt-unicode (urxvt)
!!

URxvt.geometry:             96x32
URxvt.scrollBar_right:      true
URxvt.scrollBar_floating:   true
URxvt.scrollstyle:          plain
URxvt.cursorBlink:          true
URxvt.cursorUnderline:      false
URxvt.pointerBlank:         true
URxvt.visualBell:           false
URxvt.saveLines:            3000

!! Font list and Spacing
URxvt.font:                 xft:SauceCodePro\ NF:size=13,\
                            xft:Source\ Code\ Pro:size=13,\
                            xft:Font\ Awesome\ 5\ Free,\
                            xft:Hack,\
                            xft:IPAGothic,\
                            xft:DejaVu\ Sans\ Mono,\
                            xft:Unifont


!black
*color0:              #000000
!red
*color1:              #B83019
!green
*color2:              #51BF37
!yellow
*color3:              #C6C43D
!blue
*color4:              #0C24BF
!magenta
*color5:              #B93EC1
!cyan
*color6:              #53C2C5
!white
*color7:              #C7C7C7
!bright black
*color8:              #676767
!bright red
*color9:              #EF766D
!bright green
*color10:             #8CF67A
!bright yellow
*color11:             #FEFB7E
!bright blue
*color12:             #6A71F6
!bright magenta
*color13:             #F07EF8
!bright cyan
*color14:             #8EF9FD
!bright white
*color15:             #FEFFFF

*foreground:          #C7C7C7
*background:          #000000
*scrollColor:         #504945

URxvt.inputMethod:          fcitx
URxvt.preeditType:          OnTheSpot
